<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>

<StackPane xmlns:fx="http://javafx.com/fxml" xmlns="http://javafx.com/javafx/8" fx:controller="com.mtr.fxcontroller.MainController">
    <VBox spacing="20" alignment="CENTER" prefWidth="350" prefHeight="300">
        <Label text="Select an App" style="-fx-font-size: 24px;" />
        <Button text="Play Snake Game" onAction="#openSnakeGame" />
        <Button text="Open Sort Visualizer" onAction="#openSortVisualizer" />
    </VBox>
</StackPane>

